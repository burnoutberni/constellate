{
	"name": "constellate-client",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:debug": "tsc && vite build --debug",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage --reporter=default",
		"test:storybook": "vitest run --project=storybook",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss,html}\" --ignore-path ../.prettierignore",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build -o storybook-static"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.90.12",
		"better-auth": "^1.4.9",
		"clsx": "^2.1.1",
		"dompurify": "^3.3.1",
		"leaflet": "^1.9.4",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-leaflet": "^5.0.0",
		"react-leaflet-cluster": "^4.0.0",
		"react-router-dom": "^7.11.0",
		"tailwind-merge": "^3.4.0",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.3",
		"@storybook/addon-a11y": "^10.1.10",
		"@storybook/addon-docs": "^10.1.10",
		"@storybook/addon-onboarding": "^10.1.10",
		"@storybook/addon-themes": "^10.1.10",
		"@storybook/addon-vitest": "^10.1.10",
		"@storybook/react": "^10.1.10",
		"@storybook/react-vite": "^10.1.10",
		"@tailwindcss/postcss": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/leaflet": "^1.9.21",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.50.1",
		"@typescript-eslint/parser": "^8.50.1",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.15",
		"@vitest/coverage-v8": "^4.0.15",
		"autoprefixer": "^10.4.23",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"globals": "^16.5.0",
		"jsdom": "^27.3.0",
		"playwright": "^1.57.0",
		"postcss": "^8.4.49",
		"prettier": "^3.7.4",
		"storybook": "^10.1.10",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.7.2",
		"vite": "^7.3.0",
		"vitest": "^4.0.15"
	}
}
